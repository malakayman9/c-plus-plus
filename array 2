#include<iostream>
using namespace std;
void main()
{
	bool r = false;
	const int size = 5;
	char x[size];
	int counter = 0;
	cout << "fill the array" << endl;
	for (int i = 0; i < size; i++)
	{

		cin >> x[i];
	}

	for (int i = 0; i < size; i++)
	{
		if (x[i] == 'F' || x[i] == 'f')
		{
			r = true;
			if (r == true)
			{
				cout << "found at index no. " << i << endl;
				counter++;
				
			}
			
		}
		
			
	}if (r == true)
	{
		cout << "no of f = " << counter <<endl ;
	}
	if (r == false)
	{
		cout << "not found";
	}
	
}
