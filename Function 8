#include<iostream>
using namespace std;

int arrayav(int x[],int y) 
{
	int avrage = 0;
	int sum = 0;
	for (int i = 0; i < y; i++)
	{
		sum = sum + x[i];
	}
	avrage = sum / y;
	return avrage;
}

void main() 
{
	const int size = 5;
	int array[size];
	for (int i = 0; i < size; i++)
	{
		cin >> array[i];
	}
	cout << arrayav(array,size);

}
