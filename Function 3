#include<iostream>
using namespace std;

 int add(int x[],int y)
 {
	 
	 int sum = 0;
	 for (int i = 0; i < y; i++) 
	 {
		 sum = sum + x[i];
	 }
	 return sum;

}
void main() {
	const int size = 5;
	int array1[size];
	for( int i=0 ; i<size ; i++)
	{
	cin >> array1[size];
	}
	cout << add(array1, size);


}
