#include<iostream>
#include<string>
using namespace std;


class polygon
{
public:

	int h, w;

	void setvalue(int x, int y)
	{
		h = x;
		w = y;
	}

};
class rect :public polygon
{
public:
	int area()
	{
		return h * w;
	}

};
class tri :public polygon
{
public:
	int area()
	{
		return (h * w)/2;
	}

};


void main() 
{
	int x, y;
	cin >> x, y;
	cout << "enter two numbers";
	
}
